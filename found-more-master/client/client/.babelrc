{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "Common": "../common",
        "@/graphql": "./src/graphql",
        "@": "./src/js"
      }
    }],
    "react-hot-loader/babel",
    "import-graphql",
    ["@babel/plugin-proposal-class-properties", { "loose" : true}],
    "babel-plugin-styled-components",
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-proposal-optional-chaining"
  ]
}
